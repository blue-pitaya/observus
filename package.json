{
  "name": "observus",
  "version": "0.8.0",
  "description": "Simple and minimal reactive UI library for web.",
  "author": "blue-pitaya <blue-pitaya@pm.me>",
  "keywords": [
    "observus",
    "reactive",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blue-pitaya/observus"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "lib"
  ],
  "module": "./lib/observus.js",
  "main": "./lib/observus.cjs",
  "types": "./lib/observus.d.ts",
  "exports": {
    ".": {
      "import": "./lib/observus.js",
      "require": "./lib/observus.cjs",
      "types": "./lib/observus.d.ts"
    },
    "./dom": {
      "import": "./lib/dom.js",
      "require": "./lib/dom.cjs",
      "types": "./lib/dom.d.ts"
    },
    "./helpers": {
      "import": "./lib/helpers.js",
      "require": "./lib/helpers.cjs",
      "types": "./lib/helpers.d.ts"
    },
    "./tags": {
      "import": "./lib/tags.js",
      "require": "./lib/tags.cjs",
      "types": "./lib/tags.d.ts"
    },
    "./svg-tags": {
      "import": "./lib/svg-tags.js",
      "require": "./lib/svg-tags.cjs",
      "types": "./lib/svg-tags.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3"
  }
}

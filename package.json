{
  "name": "observus",
  "version": "0.2.1",
  "description": "Simple and minimal reactive UI library for web.",
  "author": "blue-pitaya <blue-pitaya@pm.me>",
  "keywords": [
    "observus",
    "reactive",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blue-pitaya/observus"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/observus-core.js",
  "main": "./dist/observus-core.cjs",
  "types": "./dist/observus-core.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/observus-core.d.ts",
        "default": "./dist/observus-core.js"
      },
      "require": {
        "types": "./dist/observus-core.d.ts",
        "default": "./dist/observus-core.cjs"
      }
    },
    "./helpers": {
      "import": "./dist/observus-helpers.js",
      "require": "./dist/observus-helpers.cjs"
    },
    "./tags": {
      "import": {
        "types": "./dist/observus-tags.d.ts",
        "default": "./dist/observus-tags.js"
      },
      "require": {
        "types": "./dist/observus-tags.d.ts",
        "default": "./dist/observus-tags.cjs"
      }
    },
    "./attributes": {
      "import": "./dist/observus-attributes.js",
      "require": "./dist/observus-attributes.cjs"
    },
    "./svg-tags": {
      "import": "./dist/observus-svg-tags.js",
      "require": "./dist/observus-svg-tags.cjs"
    },
    "./svg-attributes": {
      "import": "./dist/observus-svg-attributes.js",
      "require": "./dist/observus-svg-attributes.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3"
  }
}
